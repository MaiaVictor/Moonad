Parse.arr: Term -> List(String) -> Parse(Term)
  (bind) (vars)
  Parse.then<Unit><Term>(Parse.space)          |
  Parse.then<String><Term>(Parse.symbol("->")) |
  let vars = List.cons<String>(String.nil)(List.cons<String>(String.nil)(vars))
  Parse.bind<Term  ><Term>(Parse.trm(vars))    | (body)
  let term =
    Term.all
    | Bool.false;
    | String.nil;
    | String.nil;
    | Term.shift(bind)(Nat.1)(Nat.0);
    | body;
  Parse.pure<Term>(term);;;
