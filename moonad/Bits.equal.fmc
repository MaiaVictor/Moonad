Bits.equal: Bits -> Bits -> Bool
  (bs1)
  bs1<() Bits -> Bool>
  | (bs2) bs2<() Bool>
    | Bool.true;
    | () Bool.false;
    | () Bool.false;
  ;
  | (bs1.pred) (bs2) bs2<() Bool>
    | Bool.false;
    | (bs2.pred) Bits.equal(bs1.pred)(bs2.pred);
    | () Bool.false;
  ;
  | (bs1.pred) (bs2) bs2<() Bool>
    | Bool.false;
    | () Bool.false;
    | (bs2.pred) Bits.equal(bs1.pred)(bs2.pred);
  ;