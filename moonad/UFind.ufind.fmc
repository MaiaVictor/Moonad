UFind.ufind: <B: Type> -> UFind -> Bits -> (Bits -> Nat -> B) -> Maybe(B)
  <B> (uf) (path) (k)
  let found = Map.lookup<UNode>(path)(uf)
  found<() Maybe(B)>
  | Maybe.none<B>;
  | (found.val)
    let node = found.val
    node<() Maybe(B)>
    | (node.rank)   Maybe.some<B>(k(path)(node.rank));
    | (node.parent) UFind.ufind<B>(uf)(node.parent)(k);;