// A Map from bitstrings to values.
// T Map<A>
// | empty
// | multi(val: Maybe(A), lft: Map(A), rgt: Map(A))
Map: (A: Type) -> Type
  (A)
  map<P: Map(A) -> Type> ->
  (empty:
    P(Map.empty<A>)) ->
  (multi:
    (val: Maybe(A)) ->
    (lft: Map(A)) ->
    (rgt: Map(A)) ->
    P(Map.multi<A>(val)(lft)(rgt))) ->
  P(map)
