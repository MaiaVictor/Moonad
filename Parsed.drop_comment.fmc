Parsed.drop_comment: String -> String
  (code)
  let code = Parsed.drop_spaces(code)
  // Removes "// ... \n"
  let code = 
    let fst2 = String.take(Nat.2)(code)
    let sla2 = String.cons(Char.sol)(String.cons(Char.sol)(String.nil))
    String.eql(fst2)(sla2)<() String>
    | let code = String.drop(Nat.2)(code)
      let code = Parsed.drop_while((x) Bool.not(U32.eql(Char.lf)(x)))(code)
      code;
    | code;
  code
